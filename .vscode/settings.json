{
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "(\\.h|\\.cpp)$",
                "cmd": "./scripts/build.sh"
            },
        ]
    },
    "testMate.cpp.discovery.loadOnStartup": true,
    "testMate.cpp.discovery.strictPattern": true,
    "C_Cpp.errorSquiggles": "enabled",
    "files.associations": {
        "iosfwd": "cpp",
        "vector": "cpp"
    },
    "C_Cpp.default.intelliSenseMode": "",
}